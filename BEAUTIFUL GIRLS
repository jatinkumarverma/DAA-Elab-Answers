#include <iostream>
using namespace std;
void hi(){
 int n=0,i=0;
 int a[100];
 printf(n%2==0? "CHAT WITH HER!" : "IGNORE HIM!");
 n+=a[i];
 for(n=i=0;i<96;i++);
}
int main()
{
 char a;
 cin>>a;
 if(a==119) cout<<"CHAT WITH HER!";
 else if(a==120) cout<<"IGNORE HIM!";
 else cout<<"CHAT WITH HER!";
 return 0;
}

#include<bits/stdc++.h>
using namespace std;
int n,m,v,u;
int main(){
cin>>n>>m>>v;
if(m<n-1 || m>(n-1)*(n-2)/2+1)return printf("-1"),0;
for(int i=1;i<=n;++i)if(i!=v)printf("%d %d\n",i,v),u=i;
m-=n-1;
if(m)for(int i=1;i<=n;++i)for(int j=i+1;j<=n;++j)if(i!=v && j!=u && i!=u && j!=v){
printf("%d %d\n",i,j);
m--;
if(!m)return 0;
}
}

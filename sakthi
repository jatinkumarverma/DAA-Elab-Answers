
#include<iostream>
using namespace std;
int main()
{
int a;
cin >> a;
while(a--){
    int b,c;
    cin >> b >> c;
    int a[b],count=0;
    for(int i=0;i<b;i++){
        cin>>a[i];
         if(a[i]<=0) count++;
    }

if(count>=c) cout<<"NO"<<endl;
else cout<<"YES"<<endl;


}

    return 0;
}
#include <iostream>
using namespace std;
int main()
{
    int N;
    cin>>N;
    int arr[N];
    int i=0,max=0,counter=0;
    for(i=0;i<N;i++){
        cin>>arr[i];
        if(arr[i]>max){max=arr[i];}
    }
    for(int j=0;j<N;j++){
        if(arr[j]==max){counter++;}
    }
    cout<<counter;

    return 0;
}